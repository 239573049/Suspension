<Window x:Class="Gotrays.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:blazor="clr-namespace:Microsoft.AspNetCore.Components.WebView.Wpf;assembly=Microsoft.AspNetCore.Components.WebView.Wpf"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="CanResizeWithGrip"
        WindowStartupLocation="CenterScreen"
        Icon="wwwroot/favicon.png"
        MinWidth="600"
        MinHeight="500"
        Title="Gotrays.Desktop" Height="450" Width="800">
    <Grid >
        <blazor:BlazorWebView Name="BlazorWeb" HostPage="wwwroot\index.html" Services="{DynamicResource services}">

        </blazor:BlazorWebView>
    </Grid>
</Window>